<UserControl x:Class="ToolBox_Pro.Views.WikiUploadView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:ToolBox_Pro.Views"
    xmlns:viewmodels="clr-namespace:ToolBox_Pro.ViewModels"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400">
    <UserControl.DataContext>
        <viewmodels:WikiUploadViewModel/>
    </UserControl.DataContext>
    <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
        <Button Content="Wiki UploadTool starten" 
                Command="{Binding StartJavaProcessCommand}" Width="300" Height="40"/>
        
    </StackPanel>

</UserControl>
