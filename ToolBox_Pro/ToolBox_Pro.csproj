  </ItemGroup>
  <ItemGroup>
    <Resource Include="Images\WN.jpg" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="Images\WN.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
